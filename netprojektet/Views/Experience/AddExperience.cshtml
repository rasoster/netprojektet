@model ExperienceViewModel
                <form asp-action="AddExperience" method="post">
    <div class="form-group">
        <label for="formGroupExampleInput">Titel</label>
        <input asp-for="experience.Name" class="form-control" type="text" />
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput">Beskrivning</label>
        <input asp-for="experience.Description" class="form-control" type="text" />
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput">Startdatum</label>
        <input asp-for="profileHasExperience.Startdate" class="form-control" type="date" />
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput">Slutdatum</label>
        <input asp-for="profileHasExperience.Enddate" class="form-control" type="date" />
    </div>
    <div class="form-group mb-4 mt-1">
    <input type="submit" value="Spara" class="btn btn-success" />
    </div>
</form>
